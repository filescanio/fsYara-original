rule SSC_File_Eraser
{
	meta:
	
	filetype = "bat"
	
	strings:
	
	$main1 = "del " nocase
	$main2 = "erase " nocase
	$s1 = "/Q" nocase
	$s2 = "/S" nocase
	$p1 = " *."
	
	condition:
	
	(any of ($main*) ) and ( any of ($s*) ) and $p1
}

rule SSC_Bat_NjRat_X
{
	meta:
	
	filetype = "bat"
	
	strings:
	
	$s1 = "ping 127.0.0.1 & del" nocase
	$s2 = "ping 0 & del" nocase
	$s3 = "ping 127.0.0.1 & erase" nocase
	$s4 = "ping 0 & erase" nocase
	
	condition:
	
	any of them
	
}


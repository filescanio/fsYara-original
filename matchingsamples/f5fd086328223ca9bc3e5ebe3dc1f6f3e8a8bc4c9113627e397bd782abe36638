            //   89742414             | test                eax, eax
            //   895c2410             | mov                 edi, eax
            //   897c2408             | mov                 edx, dword ptr [ebp - 0x4c]
            //   c744241c00000000     | jle                 0xc4

        $sequence_2 = { dee9 d9ac24a4000000 db9c24a0000000 d9ac24a6000000 8b8424a0000000 39d0 0f8e3a080000 }
